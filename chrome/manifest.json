{
  "name": "RecycleFinder Scotland",
  "description": "Find recycle points and centres near you or anywhere in Scotland",
  "version": "1",
  "manifest_version": 2,
  "app": {
    "urls": [
      "*://www.recyclefinder.co.uk"
    ],
    "launch": {
      "web_url": "http://www.recyclefinder.co.uk"
    }
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "geolocation",
    "notifications"
  ]
}